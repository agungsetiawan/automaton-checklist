<h1>Edit Project</h1>

<%= form_for(@project) do |f| %>
  <div>
    <%= f.label :name %> <br/>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :date %> <br/>
    <%= f.date_select :date %>
  </div>

  <div>
    <%= f.label :description %> <br/>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>

<%= link_to "Back", @project %>